<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<properties
		resource="net/yuanmomo/tools/db/orm/mybatis/generatorConfig.properties" />
	<context id="MBG" targetRuntime="MyBatis3" defaultModelType="conditional">
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="Example$" />
			<property name="replaceString" value="Param" />
		</plugin>
		<!-- MyBatis generator 自带的是假分页，效率很低 -->
		<plugin type="net.yuanmomo.tools.db.orm.mybatis.generator.plugin.pagination.PaginationPlugin" />
		<!-- MyBatis generator 别名插件-->
		<plugin type="net.yuanmomo.tools.db.orm.mybatis.generator.plugin.alias.AliasPlugin" />
		<!-- Mapper 接口方法声明异常抛出 -->
		<plugin type="net.yuanmomo.tools.db.orm.mybatis.generator.plugin.MapperExceptionPlugin" >
			<property name="exceptionClass" value="java.lang.RuntimeException"/>
		</plugin>
		<!-- toString 方法的实现插件 -->
		<plugin type="net.yuanmomo.tools.db.orm.mybatis.generator.plugin.ToStringPlugin" />
		
		<!-- MyBatis generator 额外java接口和xmlmaper插件 -->
<!-- 		<plugin type="net.yuanmomo.tools.db.orm.mybatis.generator.plugin.dao.DAOPlugin" > -->
<!-- 			<property name="typeName" value="DAO"/> -->
<!-- 		</plugin> -->

		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>
		
		<jdbcConnection driverClass="${driver}" connectionURL="${url}"
			userId="${user}" password="${password}">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<javaModelGenerator targetPackage="${modelPackage}"
			targetProject="${targetProject}">
			<property name="enableSubPackages" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="${sqlMapPackage}"
			targetProject="${targetProject}">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="${javaClientPackage}" targetProject="${targetProject}">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<table tableName="test" domainObjectName="Test" >
			<generatedKey column="id" sqlStatement="MySql" identity="true"/>
		</table>
<!-- 		<table tableName="user" domainObjectName="User" > -->
<!-- 			<generatedKey column="userId" sqlStatement="MySql" identity="true"/> -->
<!-- 		</table> -->
